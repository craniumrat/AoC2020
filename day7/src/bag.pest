WHITESPACE = _{ " " }

number = @{ ASCII_DIGIT+ }
adjective = @{ ASCII_ALPHA+ }
color = @{ ASCII_ALPHA+ }
inner_bag = { number ~ adjective ~ color ~ "bag" ~ "s"* }
outer_bag = { adjective ~ color ~ "bags" }
inner_bags = { inner_bag ~ ("," ~ inner_bag)* }
container = { outer_bag ~ "contain" ~ inner_bags }
